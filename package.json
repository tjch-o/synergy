{
  "name": "forum-app",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "client": "cd client && npm run dev",
    "server": "cd server && npm run start",
    "start": "concurrently \"npm run client\" \"npm run server\"",
    "build": "cd client && npm run build",
    "install_client": "cd client && npm install",
    "install_server": "cd server && npm install",
    "install": "concurrently \"npm run install_client\" \"npm run install_server\"",
    "format_client": "cd client && npm run format",
    "format_server": "cd server && npm run format",
    "format": "concurrently \"npm run format_client\" \"npm run format_server\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
